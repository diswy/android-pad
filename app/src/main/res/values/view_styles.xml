<resources>

    <!-- Base application theme. -->

    <style name="horizontalDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/color_line</item>
    </style>

    <style name="verticalDivider">
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/color_line</item>
    </style>

    <style name="btnBack">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:src">@drawable/ic_back</item>
        <item name="android:background">@null</item>
        <item name="android:onClick">backTop</item>
    </style>

</resources>
