<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FancyButton">
        <attr name="fb_shape" format="enum">
            <enum name="round_rect" value="101"/>
            <enum name="arc" value="102"/>
            <enum name="rect" value="103"/>
        </attr>
        <attr name="fb_bg_color" format="color"/>
        <attr name="fb_border_color" format="color"/>
        <attr name="fb_text_color" format="color"/>
        <attr name="fb_hover_color" format="color"/>
        <attr name="fb_text_hover_color" format="color"/>
        <attr name="fb_disabled_color" format="color"/>
        <attr name="fb_border_width" format="dimension"/>
        <attr name="fb_border_radius" format="dimension"/>
        <attr name="fb_text_size" format="dimension"/>
        <attr name="fb_text" format="string"/>
        <attr name="fb_clickable" format="boolean"/>
        <attr name="fb_showBorder" format="boolean"/>
        <attr name="fb_text_bold" format="boolean"/>
    </declare-styleable>

</resources>